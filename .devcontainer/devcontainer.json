{
    "name": "FastKubernetesBootstrap",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.profiles.linux": {
                    "zsh": {
                        "path": "/bin/zsh"
                    }
                },
                "terminal.integrated.defaultProfile.linux": "zsh",
                "editor.formatOnSave": true
            }
        },
        "extensions": [
            "esbenp.prettier-vscode"
        ]
    }
}